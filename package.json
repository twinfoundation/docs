{
	"name": "@twinfoundation/docs",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"docusaurus": "docusaurus",
		"start": "docusaurus start",
		"build": "docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve",
		"write-translations": "docusaurus write-translations",
		"write-heading-ids": "docusaurus write-heading-ids",
		"typecheck": "tsc",
		"format": "prettier --config .prettierrc --write .",
		"lint:format": "prettier --config .prettierrc --check .",
		"lint:markdown": "markdownlint docs/**/*.md --ignore docs/pkgs/**",
		"lint:spell": "cspell **/*.md --no-progress",
		"lint": "npm run lint:format && npm run lint:markdown && npm run lint:spell",
		"dist": "npm run clear && npm run build",
		"commitlint": "commitlint --edit",
		"prepare": "husky",
		"postinstall": "patch-package"
	},
	"dependencies": {
		"@docusaurus/core": "3.8.0",
		"@docusaurus/preset-classic": "3.8.0",
		"@mdx-js/react": "3.1.0",
		"clsx": "2.1.1",
		"prism-react-renderer": "2.4.1",
		"react": "19.0.0",
		"react-dom": "19.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "19.8.0",
		"@commitlint/config-conventional": "19.8.0",
		"@docusaurus/module-type-aliases": "3.8.0",
		"@docusaurus/tsconfig": "3.8.0",
		"@docusaurus/types": "3.8.0",
		"cspell": "8.17.5",
		"husky": "9.1.7",
		"markdownlint-cli": "0.44.0",
		"patch-package": "8.0.0",
		"prettier": "3.5.2",
		"typescript": "5.7.3"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 3 chrome version",
			"last 3 firefox version",
			"last 5 safari version"
		]
	},
	"engines": {
		"node": ">=18.0"
	}
}
